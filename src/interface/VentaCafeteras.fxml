<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
   <children>
      <VBox alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefWidth="200.0" spacing="25.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Articulo" />
                  <ComboBox fx:id="comboBoxArticulo" prefHeight="27.0" prefWidth="229.0" promptText="Seleccione tipo de articulo" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
            <VBox alignment="CENTER" spacing="20.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Seleccione marca/s" />
                  <HBox alignment="CENTER" prefWidth="300.0" spacing="20.0">
                     <children>
                        <CheckBox fx:id="checkBoxBosch" mnemonicParsing="false" text="Bosch" />
                        <CheckBox fx:id="checkBoxDeLonghi" mnemonicParsing="false" text="De'Longhi" />
                        <CheckBox fx:id="checkBoxJura" mnemonicParsing="false" text="Jura" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefWidth="350.0" spacing="20.0">
                     <children>
                        <CheckBox fx:id="checkBoxPhilips" mnemonicParsing="false" text="Philips" />
                        <CheckBox fx:id="checkBoxSaeco" mnemonicParsing="false" text="Saeco" />
                        <CheckBox fx:id="checkBoxSeverin" mnemonicParsing="false" text="Severin" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefWidth="350.0" spacing="20.0">
                     <children>
                        <CheckBox fx:id="checkBoxTaurus" mnemonicParsing="false" text="Taurus" />
                        <CheckBox fx:id="checkBoxUfesa" mnemonicParsing="false" text="Ufesa" />
                        <CheckBox fx:id="checkBoxKrups" mnemonicParsing="false" text="Krups" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Â¿Donde desea buscar los articulos?" />
                  <HBox alignment="CENTER" spacing="50.0">
                     <children>
                        <CheckBox fx:id="checkBoxCorteIngles" mnemonicParsing="false" text="El Corte Ingles" />
                        <CheckBox fx:id="checkBoxFnac" mnemonicParsing="false" text="Media Markt" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
            <Button fx:id="buttonBuscar" mnemonicParsing="false" text="Buscar">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <TableView fx:id="tableViewArticulos" />
   </children>
</HBox>
